{
  "name": "y2y-panel",
  "private": true,
  "scripts": {
    "start": "pnpm dev",
    "dev": "ts-node ./server",
    "prod": "pnpm build && pnpm prod:serve",
    "build": "pnpm build:routes && pnpm build:clean && pnpm build:vite && pnpm build:prerender",
    "build:clean": "rimraf dist/",
    "build:routes": "ts-node setup-routes.ts",
    "build:vite": "vite build && vite build --ssr",
    "build:prerender": "vite-plugin-ssr prerender",
    "preview": "serve dist/client/ --listen 3000",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit"
